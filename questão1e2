#include <stdio.h>
#define MAX 1024
int v[MAX]; // por quê?

int main()
{

    int n; // delimita a parte util
    printf("Digite o tamanho do vetor\n");
    scanf("%d", &n);

    if (n <= MAX) /*precisa do if*/
    {
        int vet[n];

        int tam = n-1;
        for (int i = 0; i<n; i++)
        { /*qual é mais vantajoso?*/
            vet[i] = tam;
            tam = tam - 1;
            printf("%d\n", vet[i]);
        }
    }
}
